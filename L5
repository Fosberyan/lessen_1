a = float(input("Выручка - "))
B = float(input("Расход - "))
itog = a - B
if itog > 0:
    print(f"Прибыль равно {itog} !")
    print(f"Странный расчет ренты - {itog / a:.2f}")
    persons = int(input("Сотрудников в вашей комапании? - "))
    print(f"Прибыль на одного сотрудника - {itog / persons:.2f}")
elif itog < 0:
    print(f"Убытком - {itog}")

